{
  "name": "GEMK_DEMO",
  "version": "1.0.0",
  "description": "",
  "main": "src/electron/index.js",
  "scripts": {
    "watch": "webpack --progress --profile --bail",
    "watch-login": "webpack --progress --profile --bail --config=webpack.login.config.js",
    "postinstall": "cd ./src && typings install",
    "start": "electron .",
    "serve": "webpack-dev-server --inline --progress --port 8080",
    "goto-sqlite": "cd ./node_modules/sqlite3",
    "configure-sqlite32": "node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-win32-ia32",
    "configure-sqlite64": "node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-win32-x64",
    "configure-sqliteUNIX": "node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-linux-x64",
    "configure-sqliteDARWIN": "node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-darwin-x64",
    "rebuild-sqlite32": "node-gyp rebuild --target=1.4.0 --arch=ia32 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-win32-ia32",
    "rebuild-sqlite64": "node-gyp rebuild --target=1.4.0 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-win32-x64",
    "rebuild-sqliteUNIX": "node-gyp rebuild --target=1.4.0 --arch=x64 --target_platform=linux --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-linux-x64",
    "rebuild-sqliteDARWIN": "node-gyp rebuild --target=1.4.0 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-darwin-x64",
    "pack": "electron-packager . \"kursach\" --out=bin --platform=win32 --arch=ia32 icon=src/assets/img/logo.ico --overwrite"
  },
  "author": "Tunegov Sergey",
  "license": "MIT",
  "devDependencies": {
    "@angularclass/hmr": "^1.2.1",
    "@angularclass/hmr-loader": "^3.0.2",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.5.0",
    "awesome-typescript-loader": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "electron-packager": "^8.2.0",
    "electron-prebuilt": "^1.2.2",
    "electron-rebuild": "^1.3.0",
    "es6-shim": "^0.35.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^2.0.3",
    "typescript": "2.2.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "@angular/animations": ">=2.0.0",
    "@angular/common": ">=2.0.0",
    "@angular/compiler": ">=2.0.0",
    "@angular/core": ">=2.0.0",
    "@angular/forms": ">=2.0.0",
    "@angular/http": ">=2.0.0",
    "@angular/material": "^2.0.0-alpha.9-3",
    "@angular/platform-browser": ">=2.0.0",
    "@angular/platform-browser-dynamic": ">=2.0.0",
    "@angular/router": ">=3.0.0",
    "bluebird": "^3.4.6",
    "bootstrap": "^4.0.0-alpha.4",
    "codelyzer": "^2.1.1",
    "core-js": "^2.4.1",
    "devextreme": "16.2.6",
    "devextreme-angular": "^16.2.6",
    "electron": "^1.4.3",
    "electron-reload": "^1.1.0",
    "fs-extra": "^2.1.2",
    "globalize": "^1.1.2",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jquery": "3.2.1",
    "ng2-file-upload": "^1.1.2",
    "ng2-simple-timer": "^1.2.5",
    "ng2-toastr": "^4.0.1",
    "node-uuid": "^1.4.7",
    "null-loader": "^0.1.1",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.4",
    "rxjs": "5.3.0",
    "sequelize": "^3.24.5",
    "sqlite3": "^3.1.6",
    "style-loader": "^0.16.1",
    "tslint": "^5.0.0",
    "tslint-loader": "^3.5.2",
    "zone.js": "^0.8.5"
  }
}
